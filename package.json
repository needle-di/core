    "lint": "turbo lint",
    "test": "turbo test",
    "test:ci": "turbo test:ci",
    "format": "turbo format"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.0",
    "typescript": "~5.7.0"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
